{
  "name": "trackjs-node",
  "version": "0.0.1",
  "description": "TrackJS agent for NodeJS",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "start": "run-s test:watch",
    "test": "run-s test:jest test:express",
    "test:express": "node ./test/integration/express.js",
    "test:jest": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrackJs/TrackJS-Node.git"
  },
  "author": "TrackJS <hello@trackjs.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/TrackJs/TrackJS-Node/issues"
  },
  "homepage": "https://github.com/TrackJs/TrackJS-Node#readme",
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.8",
    "express": "4.17.1",
    "jest": "24.8.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "typescript": "3.5.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
