{
  "name": "trackjs-node",
  "version": "0.0.10",
  "description": "TrackJS Error Monitoring agent for NodeJS",
  "keywords": [
    "javascript",
    "node",
    "error",
    "debug",
    "monitoring",
    "logging"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "fix": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "prettier --check \"{src,test}/**/*.ts\"",
    "start": "run-s test:watch",
    "teamcity": "node ./tools/teamcity",
    "test": "run-s test:jest test:express test:network test:transmit",
    "test:express": "node ./test/integration/express",
    "test:network": "node ./test/integration/network",
    "test:transmit": "node ./test/integration/transmit",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "version": "node ./tools/version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrackJs/TrackJS-Node.git"
  },
  "author": {
    "name": "TrackJS",
    "email": "hello@trackjs.com",
    "url": "https://trackjs.com/"
  },
  "homepage": "https://trackjs.com/",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "email": "hello@trackjs.com",
    "url": "https://github.com/TrackJs/TrackJS-Node/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.8",
    "axios": "0.19.0",
    "express": "4.17.1",
    "jest": "24.8.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "request": "2.88.0",
    "rimraf": "2.6.3",
    "simple-git": "1.116.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/test",
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
